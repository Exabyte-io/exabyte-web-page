{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "populateCreatorFields": false,
    "increments": false,
    "timestamps": false,
    "draftAndPublish": true
  },
  "pluginOptions": { },
  "attributes": {
    "category": {
      "via": "posts",
      "model": "category"
    },
    "author": {
      "private": true,
      "plugin": "users-permissions",
      "model": "user"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "organisms.page"
    },
    "section": {
      "type": "component",
      "repeatable": false,
      "component": "organisms.section",
      "required": true
    },
    "previewHorizontal": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": { }
    },
    "previewVertical": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": { }
    }
  }
}
